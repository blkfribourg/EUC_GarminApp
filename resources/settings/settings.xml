<settings>
 <setting
        propertyKey="@Properties.actionButton"
        title="@Strings.actionButton">
        <settingConfig type="list">
            <listEntry value="0">@Strings.buttonActionNone</listEntry>
            <listEntry value="1">@Strings.buttonActionLights</listEntry>
        </settingConfig>
    </setting>
    <setting
        propertyKey="@Properties.orangeColoringThreshold"
        title="@Strings.orangeColoringThreshold">
        <settingConfig
            type="numeric"
            min="0"
            max="100"
            errorMessage="@Strings.coloringThreshold_ErrorMessage" />
    </setting>
    <setting
        propertyKey="@Properties.redColoringThreshold"
        title="@Strings.redColoringThreshold">
        <settingConfig
            type="numeric"
            min="0"
            max="100"
            errorMessage="@Strings.coloringThreshold_ErrorMessage" />
    </setting>
    <setting
        propertyKey="@Properties.voltageCorrectionFactor"
        title="@Strings.voltageCorrectionFactor">
        <settingConfig
            type="numeric"
            min="0"
            max="2"
            errorMessage="@Strings.voltageCorrectionFactor_ErrorMessage" />
    </setting>
    <setting
        propertyKey="@Properties.rotationSpeed_PWM"
        title="@Strings.rotationSpeed_PWM">
        <settingConfig
            type="numeric"
            min="0"
            max="140"
            errorMessage="@Strings.rotationSpeed_PWM_ErrorMessage" />
    </setting>
   <setting
        propertyKey="@Properties.rotationVoltage_PWM"
        title="@Strings.rotationVoltage_PWM">
        <settingConfig
            type="numeric"
            min="48"
            max="136"
            errorMessage="@Strings.rotationVoltage_ErrorMessage" />
    </setting>
       <setting
        propertyKey="@Properties.powerFactor_PWM"
        title="@Strings.powerFactor_PWM">
        <settingConfig
            type="numeric"
            min="0"
            max="1"
            errorMessage="@Strings.powerFactor_PWM_ErrorMessage" />
    </setting>
</settings>