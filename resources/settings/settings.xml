<settings>
            <setting
                propertyKey="@Properties.wheelBrand"
                title="@Strings.wheelBrand">
                <settingConfig type="list">
                    <listEntry value="0">@Strings.wheelBrand0</listEntry>
                    <listEntry value="1">@Strings.wheelBrand1</listEntry>
                    <listEntry value="2">@Strings.wheelBrand2</listEntry>
                </settingConfig>
            </setting>
            <setting
                propertyKey="@Properties.recordActivityButtonMap"
                title="@Strings.recordActivityButtonMap">
                <settingConfig type="list">
                    <listEntry value="0">@Strings.none</listEntry>
                    <listEntry value="4">@Strings.startStopButton</listEntry>
                    <listEntry value="13">@Strings.middleButton</listEntry>
                    <listEntry value="8">@Strings.downButton</listEntry>
                </settingConfig>
            </setting>
            <setting
                propertyKey="@Properties.cycleLightButtonMap"
                title="@Strings.cycleLightButtonMap">
                <settingConfig type="list">
                    <listEntry value="0">@Strings.none</listEntry>
                    <listEntry value="4">@Strings.startStopButton</listEntry>
                    <listEntry value="13">@Strings.middleButton</listEntry>
                    <listEntry value="8">@Strings.downButton</listEntry>
                </settingConfig>
            </setting>
            <setting
                propertyKey="@Properties.beepButtonMap"
                title="@Strings.beepButtonMap">
                <settingConfig type="list">
                    <listEntry value="0">@Strings.none</listEntry>
                    <listEntry value="4">@Strings.startStopButton</listEntry>
                     <listEntry value="13">@Strings.middleButton</listEntry>
                     <listEntry value="8">@Strings.downButton</listEntry>
                </settingConfig>
            </setting>
            <setting
                    propertyKey="@Properties.actionQueueDelay"
                    title="@Strings.actionQueueDelay">
                    <settingConfig
                        type="numeric"
                        min="10"
                        max="400"
                        errorMessage="@Strings.actionQueueDelay_ErrorMessage" />
                </setting>
                <setting
                    propertyKey="@Properties.voltageCorrectionFactor"
                    title="@Strings.voltageCorrectionFactor">
                    <settingConfig
                        type="numeric"
                        min="0"
                        max="2"
                        errorMessage="@Strings.voltageCorrectionFactor_ErrorMessage" />
                </setting>
                <setting
                    propertyKey="@Properties.begodeCF"
                    title="@Strings.begodeCF">
                    <settingConfig
                        type="boolean"
                    />
                </setting>

                <setting
                propertyKey="@Properties.currentCorrection"
                title="@Strings.currentCorrection">
                <settingConfig type="list">
                    <listEntry value="0">@Strings.currentCorrection0</listEntry>
                    <listEntry value="1">@Strings.currentCorrection1</listEntry>
                    <listEntry value="2">@Strings.currentCorrection2</listEntry>
                </settingConfig>
            </setting>
                <setting
                    propertyKey="@Properties.speedCorrectionFactor"
                    title="@Strings.speedCorrectionFactor">
                    <settingConfig
                        type="numeric"
                        min="0"
                        max="2"
                        errorMessage="@Strings.speedCorrectionFactor_ErrorMessage" />
           
            </setting>
            <setting
                    propertyKey="@Properties.mainNumber"
                    title="@Strings.mainNumber">
                    <settingConfig type="list">
                    <listEntry value="0">@Strings.speed_str</listEntry>
                    <listEntry value="1">@Strings.PWM_str</listEntry>
                </settingConfig>
            </setting>
            <setting
                    propertyKey="@Properties.topBar"
                    title="@Strings.topBar">
                    <settingConfig type="list">
                    <listEntry value="1">@Strings.speed_str</listEntry>
                    <listEntry value="0">@Strings.PWM_str</listEntry>
                </settingConfig>
            </setting>
            <setting
                    propertyKey="@Properties.maxSpeed"
                    title="@Strings.maxSpeed">
                    <settingConfig
                        type="numeric"
                        min="0"
                        max="200"
                        errorMessage="@Strings.maxSpeed_ErrorMessage" />
           
            </setting>
                <setting
                    propertyKey="@Properties.maxTemperature"
                    title="@Strings.maxTemperature">
                    <settingConfig
                        type="numeric"
                        min="0"
                        max="120"
                        errorMessage="@Strings.maxTemperature_ErrorMessage" />
            </setting>
            <setting
                propertyKey="@Properties.updateDelay"
                title="@Strings.updateDelay">
                <settingConfig
                    type="numeric"
                    min="100"
                    max="1000"
                    errorMessage="@Strings.updateDelay_ErrorMessage" />
            </setting>
            <setting
            propertyKey="@Properties.orangeColoringThreshold"
            title="@Strings.orangeColoringThreshold">
            <settingConfig
                type="numeric"
                min="0"
                max="100"
                errorMessage="@Strings.coloringThreshold_ErrorMessage" />
            </setting>
            <setting
            propertyKey="@Properties.redColoringThreshold"
            title="@Strings.redColoringThreshold">
            <settingConfig
                type="numeric"
                min="0"
                max="100"
                errorMessage="@Strings.coloringThreshold_ErrorMessage" />
            </setting>
       
        <setting
            propertyKey="@Properties.rotationSpeed_PWM"
            title="@Strings.rotationSpeed_PWM">
            <settingConfig
                type="numeric"
                min="0"
                max="140"
                errorMessage="@Strings.rotationSpeed_PWM_ErrorMessage" />
        </setting>
        <setting
            propertyKey="@Properties.rotationVoltage_PWM"
            title="@Strings.rotationVoltage_PWM">
            <settingConfig
                type="numeric"
                min="48"
                max="136"
                errorMessage="@Strings.rotationVoltage_ErrorMessage" />
        </setting>
        <setting
            propertyKey="@Properties.powerFactor_PWM"
            title="@Strings.powerFactor_PWM">
            <settingConfig
                type="numeric"
                min="0"
                max="1"
                errorMessage="@Strings.powerFactor_PWM_ErrorMessage" />
        </setting>
  
        <setting
            propertyKey="@Properties.alarmThreshold_PWM"
            title="@Strings.alarmThreshold_PWM">
            <settingConfig
                type="numeric"
                min="0"
                max="100"
                errorMessage="@Strings.alarmThreshold_PWM_ErrorMessage" />
        </setting>
        <setting
            propertyKey="@Properties.alarmThreshold_speed"
            title="@Strings.alarmThreshold_speed">
            <settingConfig
                type="numeric"
                min="0"
                max="200"
                errorMessage="@Strings.alarmThreshold_speed_ErrorMessage" />
        </setting>
        <setting
            propertyKey="@Properties.alarmThreshold_temp"
            title="@Strings.alarmThreshold_temp">
            <settingConfig
                type="numeric"
                min="0"
                max="120"
                errorMessage="@Strings.alarmThreshold_temp_ErrorMessage" />
        </setting>
   
         <setting
            propertyKey="@Properties.activityRecordingOnStartup"
            title="@Strings.activityRecordingOnStartup">
            <settingConfig
                type="boolean"
            />
        </setting>
        <setting
            propertyKey="@Properties.activitySavingOnExit"
            title="@Strings.activitySavingOnExit">
            <settingConfig
                type="boolean"
            />
        </setting>
    
         <setting
            propertyKey="@Properties.averageMovingSpeedStatistic"
            title="@Strings.averageMovingSpeedStatistic">
            <settingConfig
                type="boolean"
            />
        </setting>
        <setting
            propertyKey="@Properties.topSpeedStatistic"
            title="@Strings.topSpeedStatistic">
            <settingConfig
                type="boolean"
            />
        </setting>
         <setting
            propertyKey="@Properties.watchBatteryConsumptionStatistic"
            title="@Strings.watchBatteryConsumptionStatistic">
            <settingConfig
                type="boolean"
            />
        </setting>
        <setting
            propertyKey="@Properties.watchBatteryStatistic"
            title="@Strings.watchBatteryStatistic">
            <settingConfig
                type="boolean"
            />
        </setting>
        <setting
            propertyKey="@Properties.tripDistanceStatistic"
            title="@Strings.tripDistanceStatistic">
            <settingConfig
                type="boolean"
            />
        </setting>
        <setting
            propertyKey="@Properties.voltageStatistic"
            title="@Strings.voltageStatistic">
            <settingConfig
                type="boolean"
            />
        </setting>
        
        <setting
            propertyKey="@Properties.debugMode"
            title="@Strings.debugMode">
            <settingConfig
                type="boolean"
            />
        </setting>
   
</settings>